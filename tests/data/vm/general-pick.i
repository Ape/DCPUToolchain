.UNIT stackRelative 
SET SP, 0
SET [0x1111], [SP+0x2222]
SET [SP+0x3333], [0x4444]
SET B, [0x1111]
SET C, [0x3333]
.ASSERT "B == 0xA2A2"
.ASSERT "C == 0xA4A4"
.ENDUNIT

.UNIT registerRelative
SET A, 0
SET [0x5555], [A+0x6666]
SET [A+0x7777], [0x8888]
SET B, [0x5555]
SET C, [0x7777]
.ASSERT "B == 0xA6A6"
.ASSERT "C == 0xA8A8"
.ENDUNIT

